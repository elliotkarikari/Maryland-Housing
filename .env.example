# Database (Railway auto-provides this)
DATABASE_URL=postgresql://user:password@host:port/maryland_atlas

# Mapbox (required for frontend)
MAPBOX_ACCESS_TOKEN=pk.your_mapbox_token_here

# Census Bureau API
CENSUS_API_KEY=your_census_api_key_here

# BLS API (optional, increases rate limits)
BLS_API_KEY=your_bls_key_here

# USASpending.gov (no key required for basic access)

# Environment
ENVIRONMENT=production
LOG_LEVEL=INFO
SENTRY_DSN=  # Optional: error tracking

# Data refresh settings
AUTO_REFRESH_ENABLED=true
REFRESH_TIMEZONE=America/New_York
